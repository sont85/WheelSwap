<h2 class="text-center">Transaction History</h2>
<div class='row'>
  <div ng-repeat="t in history">
    <div class='col-sm-6'>
      <h4>Traded My {{t.tradeAway}}</h4>
      <img class="img-responsive" ng-src='{{t.tradeAwayImageUrl}}'>
    </div>
    <div class="col-sm-6">
      <h4>To {{t.tradedWith}}</li>
      <h4>For their {{t.receive}}.</h4>
      <img class="img-responsive" ng-src='{{t.receiveImageUrl}}'>
    </div>
  </div>
</div>
